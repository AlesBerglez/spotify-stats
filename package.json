{
  "name": "spotify-stats",
  "version": "1.0.0",
  "description": "An app to see your spotify stats",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "nodemon index.js",
    "client": "cd client && npm run dev",
    "postinstall": "cd client && npm install"
  },
  "author": "Andrej Koman",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
